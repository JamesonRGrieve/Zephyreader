{
  "name": "client-framework",
  "version": "0.0.0",
  "scripts": {
    "dev": "rimraf ./.next && next dev -p 6969",
    "build": "rimraf ./.next && next build",
    "clean-build": "rimraf ./.next & rimraf ./node_nodules && npm install && npm run build && npm start",
    "compile": "tsc --jsx react-jsx --project tsconfig.compile.json && shx mkdir -p ./dist/style && npx postcss ./app/globals.css -o ./dist/style/globals.css",
    "start": "next start -p 6969",
    "docker": "docker-compose down && docker-compose pull && docker-compose build && docker-compose up -d",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "prettier": "prettier .",
    "prettier-fix": "prettier --write .",
    "fix": "prettier --write . && next lint --fix",
    "standalone:copy": "cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "standalone:run": "node ./.next/standalone/server.js",
    "standalone": "run-s build standalone:copy standalone:run",
    "storybook": "storybook dev -p 3001",
    "storybook-build": "storybook build",
    "storybook-run": "npx http-server storybook-static -p 3001",
    "storybook-deploy": "storybook build && gh-pages -d storybook-static",
    "storybook-test": "test-storybook --url http://localhost:3001 --browsers chromium,firefox,webkit"
  },
